@model IEnumerable<MusicStore.Models.Genre>
@{
    ViewBag.Title = "Index";
}

<h3>Browse Genres</h3>
<p>
    Select from @Model.Count() genres:
</p>
<ul>
    @foreach (var item in Model)
    { 
        <li>
            @*<a href="/Store/Browse?genre=@item.Name">@item.Name</a>*@
            @Html.ActionLink(@item.Name, "Browse", new { genre=@item.Name})

        </li>
    }
</ul>
